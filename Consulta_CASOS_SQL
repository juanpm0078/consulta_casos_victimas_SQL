SELECT DISTINCT
    DV.CODIGO AS CODIGO_CASO,
    DV.FEC_CREACION AS FECHA_INGRESO_SISTEMA,
    Z.COD_DA_ZONA AS TXT_NOM_ZONA,
    P.CODIGO AS COD_PROVINCIA,
    P.DESCRIPCION AS PROVINCIA,
    C.CODIGO AS COD_CANTON,
    C.DESCRIPCION AS CANTON,
    PA.CODIGO AS COD_PARROQUIA,
    PA.DESCRIPCION AS PARROQUIA,
    D.COD_AD_DISTRITO AS COD_AD_DISTRITO,
    D.NOM_DISTRITO AS DISTRITO,
    IE.COD_AMIE,
    DV.TXT_NOM_INSTITUCION,
    S.DESCRIPCION AS SOSTENIMIENTO,
    DV.TXT_TIENE_DECE,
    DV.TXT_NOM_INSTITUCION_DECE_EXT,
    DV.TXT_NOM_RECTOR,
    DV.TXT_NOMBRES_INFRACTOR,
    DV.NUM_CEDULA AS CEDULA_INFRACTOR,
    DV.NUM_EDAD_INFRACTOR,
    DV.TXT_SEXO_INFRACTOR,
    (SELECT CR.TXT_DESCRIPCION FROM ASIGNACIONES.INS_CATALOGO_R CR WHERE CR.CODIGO = DV.COD_CAT_RELA_INFRACTOR) AS RELACION_INFRACTOR,
    DV.TXT_NUM_TRA_DIS AS NUMERO_TRAMITE_DIST,
    DV.FEC_INGRESO_DENUNCIA_DIST,
    DV.TXT_NOM_PRESENTA_DENUNCIA,
    (SELECT CR.TXT_DESCRIPCION FROM ASIGNACIONES.INS_CATALOGO_R CR WHERE CR.CODIGO = DV.COD_CAT_DENO_DELITO) AS DELITO_EDUCATIVO,
    DV.FEC_INFRACCION,
    DV.STS_DENUNCIA_FISCALIA,
    DV.FEC_DENUNCIA_FISCALIA,
    DV.TXT_NUM_DENUNCIA_FIS,
    DV.TXT_NOM_CIUDAD_DENUNCIA,
    (SELECT CR.TXT_DESCRIPCION FROM ASIGNACIONES.INS_CATALOGO_R CR WHERE CR.CODIGO = DV.COD_CAT_PRESENTA_DENUNCIA) AS DELITO_FISCALIA,
    DV.TXT_NOM_PERSONA_INGRESA,
    DV.TXT_CARGO_PER_INGRE,
    DV.TXT_CELULAR AS TELEFONO_PER_INGRESA,
    DV.TXT_CORREO AS CORREO_PER_INGRESA,
    DV.TXT_NOMBRE_REVISA,
    DV.TXT_CARGO_PER_REVISA,
    DV.TXT_OBSERVACIONES,
    DV.STS_ESTADO
FROM ASIGNACIONES.INS_DENUNCIAS_VIOLENCIA DV
INNER JOIN ASIGNACIONES.INS_VICTIMAS V ON DV.CODIGO = V.COD_DENUNCIA
INNER JOIN ASIGNACIONES.INS_SEGUIMIENTO_DECE SD ON SD.COD_SEGUIMIENTO_DECE = V.COD_SEGUIMIENTO_DECE
INNER JOIN ADM.ADM_INSTITUCION_EDUCATIVA IE ON DV.COD_INS_INSTITUCION = IE.COD_INSTITUCION_EDUCATIVA
LEFT JOIN ASIGNACIONES.INS_INSTITUCION I ON IE.COD_AMIE = I.AMIE
LEFT JOIN ASIGNACIONES.INS_SOSTENIMIENTO S ON I.COD_SOSTENIMIENTO = S.CODIGO
LEFT JOIN ASIGNACIONES.INS_CIR_PAR CP ON I.COD_CIR_PAR = CP.CODIGO
LEFT JOIN ADM.ADM_CIRCUITO CI ON IE.COD_CIRCUITO_ADM = CI.COD_CIRCUITO
LEFT JOIN ADM.ADM_DISTRITO D ON CI.COD_DISTRITO = D.COD_DISTRITO
LEFT JOIN ADM.ADM_ZONA Z ON D.COD_ZONA = Z.COD_ZONA
LEFT JOIN ASIGNACIONES.INS_PARROQUIA PA ON CP.COD_PARROQUIA = PA.CODIGO
LEFT JOIN ASIGNACIONES.INS_CANTON C ON PA.COD_CANTON = C.CODIGO
LEFT JOIN ASIGNACIONES.INS_PROVINCIA P ON C.COD_PROVINCIA = P.CODIGO;
