SELECT DISTINCT
    DV.CODIGO AS CODIGO_CASO,
    V.CODIGO AS CODIGO_VICTIMA,
    V.TXT_NOM_VICTIMA,
    V.NUM_CEDULA,
    V.NUM_EDAD,
    V.TXT_SEXO,
    V.STS_DISCAPACIDAD,
    V.TXT_NOM_DISCAPACIDAD,
    G.DESCRIPCION AS GRADO,
    V.TXT_NOMBRE_REPRESENTANTE,
    V.TXT_CELULAR_REPRESENTANTE,
    V.TXT_EMAIL_REPRESENTANTE,
    SD.EXISTE_PLAN_ACOMPANAMIENTO,
    DV.STS_ESTADO AS ESTADO_CASO,
    DV.FEC_INGRESO_DENUNCIA_DIST,
    (SELECT CR.TXT_DESCRIPCION 
     FROM ASIGNACIONES.INS_CATALOGO_R CR 
     WHERE CR.CODIGO = DV.COD_CAT_DENO_DELITO) AS DELITO_EDUCATIVO,
    (SELECT CR.TXT_DESCRIPCION 
     FROM ASIGNACIONES.INS_CATALOGO_R CR 
     WHERE CR.CODIGO = DV.COD_CAT_RELA_INFRACTOR) AS RELACION_INFRACTOR,
    Z.COD_DA_ZONA AS TXT_NOM_ZONA,
    P.CODIGO AS COD_PROVINCIA,
    P.DESCRIPCION AS PROVINCIA,
    D.COD_AD_DISTRITO,
    D.NOM_DISTRITO AS DISTRITO,
    SD.EXISTE_EMBARAZO
FROM ASIGNACIONES.INS_DENUNCIAS_VIOLENCIA DV
INNER JOIN ASIGNACIONES.INS_VICTIMAS V ON DV.CODIGO = V.COD_DENUNCIA
INNER JOIN ASIGNACIONES.INS_SEGUIMIENTO_DECE SD ON V.COD_SEGUIMIENTO_DECE = SD.COD_SEGUIMIENTO_DECE
INNER JOIN ASIGNACIONES.INS_GRADO G ON V.COD_GRADO = G.CODIGO
INNER JOIN ADM.ADM_INSTITUCION_EDUCATIVA IE ON DV.COD_INS_INSTITUCION = IE.COD_INSTITUCION_EDUCATIVA
LEFT JOIN ASIGNACIONES.INS_INSTITUCION I ON IE.COD_AMIE = I.AMIE
LEFT JOIN ASIGNACIONES.INS_CIR_PAR CP ON I.COD_CIR_PAR = CP.CODIGO
LEFT JOIN ADM.ADM_CIRCUITO C ON IE.COD_CIRCUITO_ADM = C.COD_CIRCUITO
LEFT JOIN ADM.ADM_DISTRITO D ON C.COD_DISTRITO = D.COD_DISTRITO
LEFT JOIN ADM.ADM_ZONA Z ON D.COD_ZONA = Z.COD_ZONA
LEFT JOIN ASIGNACIONES.INS_PARROQUIA PQA ON CP.COD_PARROQUIA = PQA.CODIGO
LEFT JOIN ASIGNACIONES.INS_CANTON CT ON PQA.COD_CANTON = CT.CODIGO
LEFT JOIN ASIGNACIONES.INS_PROVINCIA P ON CT.COD_PROVINCIA = P.CODIGO;
